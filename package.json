{
  "name": "fcopy",
  "version": "0.0.8",
  "description": "when theres a file in a place and a new place for the file",
  "author": "Daniel Pihlstrom <sciolist.se@gmail.com>",
  "license": "BSD-2-Clause",
  "main": "lib/fcopy.js",
  "repository": {
    "url": "https://github.com/sciolist/fcopy.git"
  },
  "scripts": {
    "test": "FCOPY_NOFALLBACK=1 tap tests/*.js && rm -rf tests/tmp",
    "src-package": "node-pre-gyp package",
    "src-publish": "node-pre-gyp-github publish",
    "src-build": "node-pre-gyp install --build-from-source",
    "install": "node ./scripts/install.js"
  },
  "files": [
    "lib",
    "!lib/binding",
    "tests",
    "!tests/tmp",
    "src",
    "scripts/install.js",
    "binding.gyp"
  ],
  "binary": {
    "version": 1,
    "abis": {
      "47": "5.12.0",
      "48": "6.10.3",
      "51": "7.10.0",
      "57": "8.0.0"
    }
  },
  "dependencies": {
    "nan": "^2.6.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-fb-strict": "^20.0.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.0.1",
    "github": "^9.2.0",
    "node-gyp": "^3.6.1",
    "tap": "^10.3.2"
  }
}
